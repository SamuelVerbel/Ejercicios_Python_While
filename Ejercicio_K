candidato1 = 0
candidato2 = 0
candidato3 = 0
votantes = 0
while votantes < 100:
    voto = int(input("Ingrese su voto (1, 2, 3): "))
    if voto == 1:
        candidato1 += 1
    elif voto == 2:
        candidato2 += 1
    elif voto == 3:
        candidato3 += 1
    votantes += 1
print("Votos para el candidato 1:", candidato1)
print("Votos para el candidato 2:", candidato2)
print("Votos para el candidato 3:", candidato3)
if candidato1 > candidato2 and candidato1 > candidato3:
    print("El ganador es es candidato 1")
elif candidato2 > candidato1 and candidato2 > candidato3:
    print("El ganador es es candidato 2")
elif candidato3 > candidato1 and candidato3 > candidato2:
    print("El ganador es es candidato 3")
